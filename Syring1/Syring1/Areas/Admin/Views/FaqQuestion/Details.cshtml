@using Syring1.Areas.Admin.ViewModels.FaqQuestion
@model FaqQuestionDetailsViewModel


<div class="col-lg-12 grid-margin ">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title"> Faq Question Details</h4>
        </div>
        <div class="table-responsive">
            <table class="table">
                <thead>
                    <tr>
                        <th>Id</th>
                        <th>Title</th>
                        <th>Description</th>
                        <th>FaqCategoryId</th>

                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td>@Model.Id</td>
                        <td>@Model.Title</td>
                        <td>@Model.Description</td>
                        <td>@Model.FaqCategoryId</td>
                    </tr>
                </tbody>
            </table>
            <a asp-action="index" asp-controller="faqquestion" class="btn btn-info">Cancel</a>
        </div>
    </div>
</div>