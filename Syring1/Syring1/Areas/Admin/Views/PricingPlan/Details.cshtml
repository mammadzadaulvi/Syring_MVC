@using Syring1.Areas.Admin.ViewModels.PricingPlan
@model PricingPlanDetailsViewModel


<div class="col-lg-12 grid-margin ">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">Pricing Plan Details</h4>
        </div>
        <div class="table-responsive">
            <table class="table">
                <thead>
                    <tr>
                        <th>Id</th>
                        <th>Title</th>
                        <th>Description</th>
                        <th>Price</th>
                        <th>Skill</th>
                        <th>Status</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td>@Model.Id</td>
                        <td>@Model.Title</td>
                        <td>@Model.Description</td>
                        <td>@Model.Price</td>
                        <td>@Model.Skill</td>
                        <td>@Model.Status</td>
                    </tr>
                </tbody>
            </table>
            <a asp-action="index" asp-controller="pricingplan" class="btn btn-info">Cancel</a>
        </div>
    </div>
</div>